   <children>
      <TextField fx:id="txtfEventName" layoutX="34.0" layoutY="77.0" promptText="EventName" />
      <TextField fx:id="txtfDate" layoutX="34.0" layoutY="129.0" promptText="Date" />
      <TextField fx:id="txtfStartTime" layoutX="34.0" layoutY="180.0" promptText="StartTime" />
      <TextField fx:id="txtfNotes" layoutX="34.0" layoutY="283.0" promptText="Notes" />
      <Button fx:id="btnCreateNewLocation" layoutX="229.0" layoutY="231.0" mnemonicParsing="false" onAction="#handleCreateNewLocation" text="Create New Location" />
